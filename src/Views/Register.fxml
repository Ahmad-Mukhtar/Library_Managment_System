<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="RegisterFrame" prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #8b4513;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.RegistrationController">
   <Label alignment="CENTER" layoutX="289.0" layoutY="29.0" text="Register" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="36.0" />
      </font>
   </Label>
   <Button alignment="CENTER" layoutX="289.0" layoutY="413.0" mnemonicParsing="false" onAction="#registerButtonClick" prefHeight="35.0" prefWidth="122.0" style="-fx-background-color: #1E90FF; -fx-background-radius: 20px;" text="Register" textFill="WHITE" />
   <ImageView fitHeight="71.0" fitWidth="74.0" layoutX="311.0" layoutY="91.0" pickOnBounds="true">
      <Image url="@../Images/bicon.jpg" />
   </ImageView>
   <Label layoutX="14.0" layoutY="469.0" text="Already a User?" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <Hyperlink fx:id="Loginlink" layoutX="104.0" layoutY="465.0" onAction="#alreadyAUser" text="Click Here...." textFill="DODGERBLUE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Hyperlink>
   <TextField fx:id="Firstname" layoutX="96.0" layoutY="190.0" style="-fx-background-radius: 25px;" />
   <Label layoutX="26.0" layoutY="194.0" text="First Name" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <Label layoutX="379.0" layoutY="194.0" text="Last Name" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <TextField fx:id="Lastname" layoutX="450.0" layoutY="190.0" style="-fx-background-radius: 25px;" />
   <Label layoutX="26.0" layoutY="250.0" text="User Name" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <TextField fx:id="Username" layoutX="96.0" layoutY="246.0" style="-fx-background-radius: 25px;" />
   <Label layoutX="385.0" layoutY="251.0" text="Password" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <PasswordField fx:id="Password" layoutX="450.0" layoutY="246.0" style="-fx-background-radius: 25px;" />
   <Label layoutX="57.0" layoutY="307.0" text="Email" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <TextField fx:id="Email" layoutX="96.0" layoutY="302.0" style="-fx-background-radius: 25px;" />
   <DatePicker fx:id="Dob" layoutX="450.0" layoutY="302.0" />
   <Label layoutX="365.0" layoutY="306.0" text="Date of Birth" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <Label layoutX="44.0" layoutY="363.0" text="Gender" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </Label>
   <RadioButton fx:id="Male" layoutX="102.0" layoutY="363.0" mnemonicParsing="false" text="Male" textFill="WHITE">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
      <toggleGroup>
         <ToggleGroup fx:id="GenderGroup" />
      </toggleGroup>
   </RadioButton>
   <RadioButton fx:id="Female" layoutX="170.0" layoutY="363.0" mnemonicParsing="false" text="Female" textFill="WHITE" toggleGroup="$GenderGroup">
      <font>
         <Font name="Calibri Bold" size="14.0" />
      </font>
   </RadioButton>
   <Circle fx:id="circle1" fill="DODGERBLUE" layoutX="270.0" layoutY="391.0" radius="100.0" scaleX="0.03" scaleY="0.03" scaleZ="0.03" smooth="false" stroke="BLACK" strokeType="INSIDE" visible="false" />
   <Circle fx:id="circle2" fill="DODGERBLUE" layoutX="290.0" layoutY="391.0" radius="100.0" scaleX="0.03" scaleY="0.03" scaleZ="0.03" stroke="BLACK" strokeType="INSIDE" visible="false" />
   <Circle fx:id="circle3" fill="DODGERBLUE" layoutX="310.0" layoutY="391.0" radius="100.0" scaleX="0.03" scaleY="0.03" scaleZ="0.03" stroke="BLACK" strokeType="INSIDE" visible="false" />
   <Circle fx:id="circle4" fill="DODGERBLUE" layoutX="330.0" layoutY="391.0" radius="100.0" scaleX="0.03" scaleY="0.03" scaleZ="0.03" stroke="BLACK" strokeType="INSIDE" visible="false" />
</AnchorPane>
